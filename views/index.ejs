<div class="title-heading">
  <h1 class="bigBold">Super Good Club Website</h1>
  <p class="smallBold">Never get lost in the sauce again!</p>
</div>
<section class="articles">
  <h2 class="articles__heading">Latest Posts</h2>
  
    <ul class="article-ul">
      <% data.forEach(post => { %>
        <li>
          <!-- <a href="/claim/<%= post._id %>" class="newbtn">Add</a> -->
          <a href="/post/<%= post._id %>">
            <span><%= post.title %></span>
            <span class="article-list__date"><%= post.createdAt.toDateString() %></span>
          </a>
        </li>
      <% }) %>
    </ul>
  
    <% if (nextPage !== null) { %>
      <a href="/?page=<%= nextPage %>" class="pagination">&lt; View Older Posts</a>
    <% } %>

    <% if (current >= 2) { %>
     <a href="/?page=<%= current-1 %>" class="pagination">&gt; View Newer Posts</a>
    <% } %>

</section>